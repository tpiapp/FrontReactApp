{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-alert": 0,
        "no-bitwise": 0,
        "camelcase": 1,
        "curly": 1,
        "eqeqeq": 0,
        "no-eq-null": 0,
        "guard-for-in": 1,
        "no-empty": 1,
        "no-use-before-define": 0,
        "no-obj-calls": 2,
        "no-unused-vars": 0,
        "new-cap": 1,
        "no-shadow": 0,
        "strict": 1,
        "no-invalid-regexp": 2,
        "comma-dangle": 2,
        "no-undef": 2,
        "no-new": 1,
        "no-extra-semi": 1,
        "no-debugger": 2,
        "no-caller": 1,
        "semi": 2,
        "quotes": 0,
        "no-unreachable": 2
    },
    "globals": {
        "jQuery": true,
        "$": true
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    }
}